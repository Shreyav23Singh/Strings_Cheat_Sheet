//Counting  frequency of a character in a string 
class Main {
  public static void main(String[] args) {
    String str="At any cost I want Success";
    int count=0;
    for(int i=0;i<=str.length()-1;i++){
      if(str.charAt(i)=='c'){
        count++;
    }
    }
    System.out.println("frequency of c is :"+count);
    
   
  }
}
